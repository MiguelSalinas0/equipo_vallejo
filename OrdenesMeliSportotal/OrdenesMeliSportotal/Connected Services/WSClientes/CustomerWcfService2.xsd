      <xs:element minOccurs="0" name="DatabaseId" nillable="true" type="xs:string">
        <xs:annotation>
          <xs:documentation>Identifier of the database to use in this execution context.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:element xmlns:q2="http://www.cegid.fr/Retail/1.0" name="RetailContext" nillable="true" type="q2:RetailContext" />
