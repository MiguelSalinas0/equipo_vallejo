            <xs:documentation>Text to pass to test consumption.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element xmlns:q1="http://www.cegid.fr/Retail/1.0" minOccurs="0" name="clientContext" nillable="true" type="q1:RetailContext" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:complexType name="RetailContext">
